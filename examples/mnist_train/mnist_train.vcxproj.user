<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="15.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x64'">
    <LocalDebuggerCommand>$(ProjectDir)../../bin/cpu/$(TargetName)d$(TargetExt)</LocalDebuggerCommand>
    <LocalDebuggerWorkingDirectory>$(ProjectDir)../../bin/cpu</LocalDebuggerWorkingDirectory>
    <DebuggerFlavor>WindowsLocalDebugger</DebuggerFlavor>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|x64'">
    <LocalDebuggerCommand>$(ProjectDir)../../bin/cpu/$(TargetName)$(TargetExt)</LocalDebuggerCommand>
    <LocalDebuggerWorkingDirectory>$(ProjectDir)../../bin/cpu</LocalDebuggerWorkingDirectory>
    <DebuggerFlavor>WindowsLocalDebugger</DebuggerFlavor>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='ReleaseGPU|x64'">
    <LocalDebuggerCommand>$(ProjectDir)../../bin/gpu/$(TargetName)$(TargetExt)</LocalDebuggerCommand>
    <LocalDebuggerWorkingDirectory>$(ProjectDir)../../bin/gpu</LocalDebuggerWorkingDirectory>
    <DebuggerFlavor>WindowsLocalDebugger</DebuggerFlavor>
  </PropertyGroup>
</Project>